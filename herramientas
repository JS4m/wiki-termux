if [ -d /data/data/com.termux/files/home/termux/menu ]; then
					     
  conf_dir="/data/data/com.termux/files/home/wiki-termux/menu"
	     
elif [ -d /termux/menu ]; then
  conf_dir="/wiki-termux/menu"
elif [ -d /menu ]; then
  conf_dir="/menu"
fi

if [ -e /usr/lib/sudo ]; then
	sudo python3 $conf_dir/menu_3/exa
else					    
	cd /data/data/com.termux/files/home/wiki-termux/menu/menu_3
	clear
	python3 exa
fi
exit

